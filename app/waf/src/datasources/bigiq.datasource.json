{
  "connector": "rest",
  "options": {
    "headers": {
      "accept": "application/json",
      "content-type": "application/json"
    },
    "strictSSL": false
  },
  "operations": [
    {
      "template": {
        "method": "POST",
        "url": "https://{host}:{port}/mgmt/shared/authn/login",
        "body": {
          "username":"{username}",
          "password":"{password}"
        }
      },
      "functions": {
        "login": [
          "host",
          "port",
          "username",
          "password"
        ]
      }
    },
    {
      "template": {
        "method": "POST",
        "url": "https://{host}:{port}/mgmt/cm/device/tasks/licensing/pool/member-management",
        "headers": {
          "X-F5-Auth-Token": "{token}"
        },
        "body": {
          "licensePoolName": "{pool}",
          "command": "revoke",
          "address": "{address}",
          "user": "wrong",
          "password": "wrong"
        }
      },
      "functions": {
        "revoke": [
          "host",
          "port",
          "token",
          "pool",
          "address"
        ]
      }
    },
    {
      "template": {
        "method": "GET",
        "url": "https://{host}:{port}/mgmt/cm/device/tasks/licensing/pool/member-management/{task}",
        "headers": {
          "X-F5-Auth-Token": "{token}"
        }
      },
      "functions": {
        "query": [
          "host",
          "port",
          "token",
          "task"
        ]
      }
    }
  ]
}
